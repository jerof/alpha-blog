<h1>Edit article</h1>

<%= form_for @article do |f| %>
  <div class="form-group">
    <%= f.label :title %> <br>
    <%= f.text_field :title %>
  </div>
  <div class="form-group">
    <%= f.label :description %> <br>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
